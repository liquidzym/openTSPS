package com.lab.events {		import flash.events.Event;			public class TSPSDataEvent extends Event	{		private var _data:Object;				/**		 * Triggered when the connection object has finished formatting the incoming TSPS data.		 */		public static const DATA_READY:String = "dataReady";				public function TSPSDataEvent(type:String, data:Object=null, bubbles:Boolean = false, cancelable:Boolean = false)		{			_data = data;						super(type, bubbles, cancelable);					}				public override function clone():Event		{			return new TSPSDataEvent ( type, _data, bubbles, cancelable );		}				/**		 * An object containing the incoming TSPS data.		 */		public function get data():Object {			return _data;		}				/**		 * An object containing the incoming TSPS data.		 */		public function set data(value:Object):void {			_data = value;		}	}}