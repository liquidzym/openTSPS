package com.lab.events {		import flash.events.Event;			public class SocketMessageEvent extends Event {		private var _data:Array;				/**		 * Triggered when a new packet has been received via TCP from the openTSPS application		 */		public static const MESSAGE_RECEIVED:String = "messageReceived";				public function SocketMessageEvent(type:String, data:Array=null, bubbles:Boolean = false, cancelable:Boolean = false)		{			_data = data;						super(type, bubbles, cancelable);					}				public override function clone():Event		{			return new SocketMessageEvent ( type, _data, bubbles, cancelable );		}				/**		 * An array containing a packet of incoming TCP TSPS data. This may contain data for multiple people.		 */		public function get data():Array {			return _data;		}				/** 		 * An array containing a packet of incoming TCP TSPS data. This may contain data for multiple people.		 */		public function set data(value:Array):void {			_data = value;		}	}}